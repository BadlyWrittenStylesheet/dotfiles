{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small",
    "width": 21,
    // "logo": "small",
    "padding": {
      "top": 3,
      "left": 0,
      "right": 2,
    },
  },
    "display": {
      "separator": " ",
    },
    "modules": [
      {
        "type": "custom",
        "format": "{#1}     |\\__\/,|  (`\\"
      },
      {
        "type": "custom",
        "format": "{#1}   _.|o o  |_  ) )"
      },
      {
        "type": "title",
        "format": "{#1}┬─(((───(((─ {user-name-colored}@{host-name-colored} " //"╶ - . " // "🮤" // "───────╮"
      },
      // {
      //   "type": "title",
      //   "format": "{#1}╭────────── {user-name-colored}@{host-name-colored} ───────────╮"
      // },
      {
        "type": "os",
        "key": "{#separator}   {#keys}󰍹 ",
      },
      {
        "type": "kernel",
        "key": "{#separator}   {#keys}󰒋 ",
      },
      {
        "type": "packages",
        "key": "{#separator}   {#keys}󰏖 ",
        "format": "{all}",
      },
      {
        "type": "uptime",
        "key": "{#separator}   {#keys}󰅐 ",
      },
      {
        "type": "custom",
        "format": "{#1} ",
      },
      {
        "type": "cpu",
        "key": "{#separator}   {#keys}󰻠 ",
      },
      {
        "type": "memory",
        "key": "{#separator}   {#keys}󰍛 ",
      },
      // Footer
      {
        "type": "custom",
        "format": "{#1}┴─╴                                     ╶─┴" //"【●",
      },
    ],
}
